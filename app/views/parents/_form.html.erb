<%= form_for @parent do |f| %>
  <%= f.text_field :parent_name, placeholder: "Name" %><br>
  <%= f.text_field :child_name, placeholder: "Child's Name" %><br>
  <%= f.text_field :email, placeholder: "Email" %><br>
  <%= f.password_field :password, placeholder: "Password" %><br>
  <%= f.collection_select :teacher_id, Teacher.all, :id, :name %><br>
  <%= f.submit "Submit"%>
<% end %>
